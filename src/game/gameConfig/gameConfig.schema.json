{
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "const": "v0"
    },
    "playerStartModList": {
      "items": {
        "$ref": "#/definitions/PlayerStartMod"
      },
      "type": "array"
    },
    "enemyBaseLifeList": {
      "description": "Enemy life for each level starting at level 1. This will determine the max level. Max Level == array.length + 1.",
      "items": {
        "$ref": "#/definitions/EnemyBaseLife"
      },
      "type": "array"
    },
    "enemyBaseCountList": {
      "description": "Number of enemies for each level starting at level 1",
      "items": {
        "$ref": "#/definitions/EnemyBaseCount"
      },
      "type": "array"
    },
    "enemyList": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Enemy"
      }
    },
    "ascension": {
      "$ref": "#/definitions/Ascension"
    },
    "components": {
      "type": "object",
      "properties": {
        "guilds": {
          "$ref": "#/definitions/Guilds"
        },
        "skills": {
          "$ref": "#/definitions/Skills"
        },
        "weapon": {
          "$ref": "#/definitions/Weapon"
        },
        "artifacts": {
          "$ref": "#/definitions/Artifacts"
        },
        "achievements": {
          "$ref": "#/definitions/Achievements"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": [
    "ascension",
    "enemyBaseCountList",
    "enemyBaseLifeList",
    "enemyList",
    "playerStartModList",
    "version"
  ],
  "definitions": {
    "Enemy": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "level": {
          "type": "integer",
          "minimum": 0,
          "default": 1
        },
        "weight": {
          "type": "integer",
          "minimum": 0,
          "default": 100
        },
        "modList": {
          "items": {
            "$ref": "#/definitions/EnemyMod"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "name"
      ]
    },
    "Id": {
      "type": "string",
      "pattern": "^[a-f0-9]{6}$",
      "defaultSnippets": [
        {
          "body": "${RANDOM_HEX}"
        }
      ]
    },
    "Ascension": {
      "type": "object",
      "properties": {
        "trial": {
          "$ref": "#/definitions/AscensionTrial"
        },
        "ascensionInstanceList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AscensionInstance"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "trial"
      ]
    },
    "AscensionTrial": {
      "type": "object",
      "properties": {
        "timeout": {
          "type": "integer",
          "minimum": 0,
          "description": "Time in seconds before trial fails"
        },
        "enemyCount": {
          "type": "integer",
          "minimum": 1,
          "default": 1
        },
        "enemyList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AscensionEnemy"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "enemyCount",
        "enemyList"
      ]
    },
    "AscensionEnemy": {
      "type": "object",
      "properties": {
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "id": {
          "$ref": "#/definitions/Id"
        },
        "weight": {
          "type": "integer",
          "minimum": 0,
          "default": 100
        },
        "modList": {
          "items": {
            "$ref": "#/definitions/EnemyMod"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "name"
      ]
    },
    "AscensionInstance": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "modList": {
          "description": "these modifiers will be applied upon ascending",
          "items": {
            "$ref": "#/definitions/AscensionMod"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "modList"
      ]
    },
    "Guilds": {
      "type": "object",
      "properties": {
        "requirements": {
          "$ref": "#/definitions/Requirements"
        },
        "guildList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Guild"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "guildList"
      ]
    },
    "Requirements": {
      "type": "object",
      "properties": {
        "curLevel": {
          "type": "integer",
          "minimum": 0,
          "default": 1
        },
        "maxLevel": {
          "type": "integer",
          "minimum": 0,
          "default": 1
        },
        "ascensionCount": {
          "type": "integer",
          "minimum": 0,
          "default": 1
        }
      },
      "additionalProperties": false
    },
    "Guild": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "activityList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/GuildActivity"
          }
        },
        "levels": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "exp": {
                "type": "integer",
                "minimum": 1,
                "default": 0
              },
              "modList": {
                "items": {
                  "$ref": "#/definitions/PlayerMod"
                },
                "type": "array"
              }
            },
            "additionalProperties": false,
            "required": [
              "modList"
            ]
          }
        },
        "classes": {
          "$ref": "#/definitions/GuildClasses"
        }
      },
      "additionalProperties": false,
      "required": [
        "activityList",
        "classes",
        "id",
        "levels",
        "name"
      ]
    },
    "GuildActivity": {
      "type": "object",
      "properties": {
        "name": {
          "$ref": "#/definitions/GuildActivityName"
        },
        "multiplier": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "multiplier",
        "name"
      ]
    },
    "GuildActivityName": {
      "description": "Combat is disabled while performing an activity\r\n- Training - Attack skills gain exp faster\r\n- Exploring - Finds artifacts passively.\r\n- Meditation - Increased mana/mana regen and passive skills gain exp faster (auras are active while meditating)",
      "enum": [
        "Exploring",
        "Meditation",
        "Training"
      ],
      "type": "string"
    },
    "GuildClasses": {
      "type": "object",
      "properties": {
        "classList": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "$ref": "#/definitions/Id"
              },
              "name": {
                "pattern": "^[A-Za-z 0-9]{3,32}$",
                "type": "string"
              },
              "modList": {
                "items": {
                  "$ref": "#/definitions/PlayerMod"
                },
                "type": "array"
              }
            },
            "additionalProperties": false,
            "required": [
              "id",
              "modList",
              "name"
            ]
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "classList"
      ]
    },
    "Skills": {
      "type": "object",
      "properties": {
        "attackSkills": {
          "type": "object",
          "properties": {
            "attackSkillList": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AttackSkill"
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "attackSkillList"
          ]
        },
        "auraSkills": {
          "type": "object",
          "properties": {
            "requirements": {
              "$ref": "#/definitions/Requirements"
            },
            "auraSkillList": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AuraSkill"
              }
            },
            "auraSkillSlotList": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AuraSkillSlot"
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "auraSkillList",
            "auraSkillSlotList"
          ]
        },
        "passiveSkills": {
          "type": "object",
          "properties": {
            "insightCapacityEnhancerList": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "pattern": "^[A-Za-z 0-9]{3,32}$",
                    "type": "string"
                  },
                  "insight": {
                    "type": "integer",
                    "minimum": 0
                  },
                  "probabilities": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  },
                  "flavourText": {
                    "pattern": "^[A-Za-z .,!?0-9]{3,128}$",
                    "type": "string"
                  }
                },
                "additionalProperties": false,
                "required": [
                  "insight",
                  "name",
                  "probabilities"
                ]
              }
            },
            "passiveSkillList": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/PassiveSkill"
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "insightCapacityEnhancerList",
            "passiveSkillList"
          ]
        }
      },
      "additionalProperties": false
    },
    "AttackSkill": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "manaCost": {
          "type": "integer",
          "minimum": 0
        },
        "attackSpeed": {
          "default": 1,
          "minimum": 0.1,
          "type": "number"
        },
        "attackEffectiveness": {
          "type": "integer",
          "minimum": 0,
          "default": 100
        },
        "modList": {
          "items": {
            "$ref": "#/definitions/PlayerMod"
          },
          "type": "array"
        },
        "probability": {
          "type": "integer",
          "minimum": 1,
          "default": 100,
          "description": "Percent = 1/Probability"
        },
        "exp": {
          "type": "integer",
          "minimum": 1,
          "default": 0,
          "description": "1 exp gained per attack"
        }
      },
      "additionalProperties": false,
      "required": [
        "attackEffectiveness",
        "attackSpeed",
        "id",
        "manaCost",
        "modList",
        "name"
      ]
    },
    "AuraSkill": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "manaCost": {
          "type": "integer",
          "minimum": 0
        },
        "baseDuration": {
          "type": "integer",
          "minimum": 0
        },
        "modList": {
          "items": {
            "$ref": "#/definitions/PlayerMod"
          },
          "type": "array"
        },
        "probability": {
          "type": "integer",
          "minimum": 1,
          "default": 100,
          "description": "Percent = 1/Probability"
        },
        "exp": {
          "type": "integer",
          "minimum": 0
        }
      },
      "additionalProperties": false,
      "required": [
        "baseDuration",
        "id",
        "manaCost",
        "modList",
        "name"
      ]
    },
    "AuraSkillSlot": {
      "type": "object",
      "properties": {
        "requirements": {
          "$ref": "#/definitions/Requirements"
        }
      },
      "additionalProperties": false
    },
    "PassiveSkill": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "pattern": "^[A-Za-z 0-9]{3,32}$",
          "type": "string"
        },
        "insight": {
          "type": "integer",
          "minimum": 0
        },
        "modList": {
          "items": {
            "$ref": "#/definitions/PlayerMod"
          },
          "type": "array"
        },
        "probability": {
          "type": "integer",
          "minimum": 1,
          "default": 100,
          "description": "Percent = 1/Probability"
        },
        "exp": {
          "type": "integer",
          "minimum": 0
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "insight",
        "modList",
        "name"
      ]
    },
    "Weapon": {
      "type": "object",
      "properties": {
        "weaponTypeList": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/WeaponType"
          }
        },
        "modLists": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/WeaponMod"
            }
          }
        },
        "crafting": {
          "type": "object",
          "properties": {
            "advReforgeRequirements": {
              "$ref": "#/definitions/Requirements"
            },
            "craftList": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WeaponCraft"
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "craftList"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "crafting",
        "modLists"
      ]
    },
    "WeaponType": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "name": {
          "$ref": "#/definitions/WeaponTypeName"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "name"
      ]
    },
    "WeaponTypeName": {
      "enum": [
        "One Handed Sword",
        "Staff",
        "Two Handed Axe",
        "Wand"
      ],
      "type": "string"
    },
    "WeaponMod": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/Id"
        },
        "level": {
          "type": "integer",
          "minimum": 0,
          "default": 1
        },
        "weight": {
          "type": "integer",
          "minimum": 0,
          "default": 100
        },
        "mod": {
          "$ref": "#/definitions/PlayerMod"
        },
        "weaponTypes": {
          "type": "array",
          "items": {
            "enum": [
              "One Handed Sword",
              "Staff",
              "Two Handed Axe",
              "Wand"
            ],
            "type": "string"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "level",
        "mod",
        "weight"
      ]
    },
